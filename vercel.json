{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/**": {
      "runtime": "nodejs20.x",
      "maxDuration": 300,
      "memory": 1024,
      "includeFiles": "tessdata/**"
    }
  },
  "crons": [
    {
      "path": "/api/recognize-card?warmup=1",
      "schedule": "*/10 * * * *"
    }
  ]
}